<%= link_to "New deck", new_deck_path %>
<%= link_to "Sign out", session_path, method: :delete %>
<% @decks.each do |deck| %>
	<div>
		<%= link_to deck.name, deck_path(deck) %>
		-
		<%= link_to "Edit", edit_deck_path(deck)%>
		<%= link_to "Delete", deck_path(deck), method: :delete, :confirm => "Are you sure?" %>
	</div>
<% end %>